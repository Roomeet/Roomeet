{
  "name": "roomeet",
  "version": "1.0.0",
  "description": "An app that makes the search after the perfect roomate a fun and interective journey.",
  "engines": {
    "node": "14.7.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "cd client && npm run build",
    "install": "cd client && npm install && cd ../server && npm install && cd ../proxy-server && npm install && cd ../communication && npm install",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "communication": "cd communication && npm start",
    "proxy-server": "cd proxy-server && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roomeet/Roomeet.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Roomeet/Roomeet/issues"
  },
  "homepage": "https://github.com/Roomeet/Roomeet#readme",
  "dependencies": {
    "concurrently": "^5.3.0"
  }
}
